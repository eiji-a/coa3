<tr>
  <td>
    <%= link_to image_tag('unlink12.gif'), :action => 'unlink',
    :id => @content, :parent_id => @content.id, :child_id => child.id %>
    <%= image_tag('delete12.gif') %>
  </td>
  <td><%= link_to child.title, :id => child.id %></td>
  <td>
    <% if child.folder != nil %>
    <%= link_to child.folder.name, :controller => 'folders', :action => 'show', :id => child.folder.id %>
    <% end %>
  </td>
  <td><%= child.created_at.strftime("%Y-%m-%d") %></td>
</tr>
